      "events": [
        {
          "name": "Build Teams and Roles Checkboxes",
          "type": "Load",
          "action": "Custom",
          "code": "var rolesElement = $('#rolesCheckbox')\nvar teamsElement = $('#teamsCheckbox')\n\nK.api(\"GET\",\"app/api/v1/teams\",{\n\tsuccess: function(data){\n\t\tteams = data.teams;\n\t\t_.each(teams, function(team){\n\t\t\tvar memberships = K('field[Memberships]').value();\n\t\t\tvar checked = \"\";\n\t\t\tif (!_.isEmpty(memberships)){\n\t\t\t\tmemberships = JSON.parse(memberships)\n\t\t\t\t_.includes(memberships, team.name) ? checked = \"checked\" : \"\";\n\t\t\t}\n\t\t\tif (team.name.startsWith('Role::')){\n\t\t\t\tvar label = (team.name).substring((team.name).lastIndexOf(\"::\") + 2)\n\t\t\t\t$('<div class=\"checkbox\"><label><input ' + checked + ' class=\"roleCheckbox\" type=\"checkbox\" value=\"' + team.name + '\">' + label + '</label></div>').appendTo(rolesElement);\n\t\t\t} else if (!team.name.startsWith('Role')){\n\t\t\t\t$('<div class=\"checkbox\"><label><input ' + checked + ' class=\"teamCheckbox\" type=\"checkbox\" value=\"' + team.name + '\">' + team.name + '</label></div>').appendTo(teamsElement);\n\t\t\t}\n\t\t});\n\t\t// If membership field is populated, assume it's a review request and disable all checkbox inputs\n\t\tvar memberships = K('field[Memberships]').value();\n\t\tif (!_.isEmpty(memberships)){\n\t\t\t$('#rolesCheckbox input, #teamsCheckbox input').attr('disabled','disabled');\n\t\t}\n\t}\n});",
          "runIf": ""
        },
        {
          "name": "Save Roles and Teams Checkbox Questions",
          "type": "Submit",
          "action": "Custom",
          "code": "// Set Memberships Question on Submit\nvar memberships = [];\n$('.roleCheckbox, .teamCheckbox').each(function(){\n\tvar val = $(this).val();\n\tif (this.checked){\n\t\tmemberships.push(val);\n\t}\n})\nK('field[Memberships]').value(JSON.stringify(memberships));"
        }
      ],
      "name": "Page One",
      "renderType": "submittable",
      "type": "page"
    }
