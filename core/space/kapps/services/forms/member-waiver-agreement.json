          "code": "K('bridgedResource[Members]').load({\n  attributes: ['Member ID', 'First Name', 'Last Name', 'Covid19 Waiver'],\n  success: function(bridgedData) {\n debugger;\n    var options=[];\n    $.each( bridgedData, function( key, value ) {\n      if (value['Covid19 Waiver']===null || value['Covid19 Waiver']===\"\" || value['Covid19 Waiver']===\"NOT Agreed\") {\n\t      options[options.length]={'label':value['First Name']+\" \"+value['Last Name'],'value':value['id']};\n      }\n    });\n\t\tK('field[Members]').options(options);\n    bundle.config.widgets.selectMenu({\n      element: K(\"form\").find(\"#selectMemberMenu\")[0],\n      options: options,\n      onChange: function(option) {\n        K('field[Members]').value(option.value);\n        setTimeout(function(){\n          $(K(\"form\").find(\"#selectMemberMenu .css-1492t68\")).html(option.label);\n        },100);\n      },\n      value: K('field[Members]').value(),\n    });  \n  }\n});\n",
          "runIf": "!form('review')"
        },
        {
          "name": "Load Review",
          "type": "Load",
          "action": "Custom",
          "code": "K('section[Waiver Section]').show();\nK('section[Member Select Space]').hide();    \n\n$(\".agree\").prop('disabled', true)\n$(\".notagree\").prop('disabled', true)\n\nif (K('field[Agreement]').value()===\"Agreed\") {\n  $(\".notagree\").hide();\n} else {\n  $(\".agree\").hide();\n}\n",
          "runIf": "form('review')"
        },
